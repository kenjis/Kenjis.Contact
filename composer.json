{
    "name": "kenjis/contact",
    "description":"BEAR.Sunday Sample Contact Form",
    "license": "MIT",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Tavicu/bootstrap-confirmation.git"
        }
    ],
    "require": {
        "bear/package": "0.12.*",
        "swiftmailer/swiftmailer": "@stable",
        "tavicu/bootstrap-confirmation": "dev-master"
    },
    "require-dev": {
        "bear/dev-package": "~0.1@dev"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Kenjis\\Contact\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    }
}
